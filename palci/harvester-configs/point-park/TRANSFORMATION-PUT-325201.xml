            &lt;/xsl:when&gt;&#xD;
            &lt;xsl:otherwise&gt;&#xD;
            &lt;xsl:value-of select="$input"/&gt;&#xD;
            &lt;/xsl:otherwise&gt;&#xD;
        &lt;/xsl:choose&gt;&#xD;
    &lt;/xsl:template&gt;&#xD;
&#xD;
&lt;/xsl:stylesheet&gt;</script>
