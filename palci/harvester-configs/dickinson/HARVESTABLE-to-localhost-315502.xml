                      &lt;/i&gt;&#xD;
                    &lt;/arr&gt;&#xD;
                  &lt;/notes&gt;&#xD;
                  &lt;items&gt;&#xD;
                    &lt;arr&gt;&#xD;
                    &lt;xsl:for-each select="../marc:datafield[@tag='999']/marc:subfield[@code='l'][.=$loc-clean]/.."&gt;&#xD;
                      &lt;i&gt;&#xD;
