            &lt;xsl:for-each select="marc:datafield[@tag='999']"&gt;&#xD;
              &lt;xsl:sort select="./marc:subfield[@code='l']"/&gt;&#xD;
              &lt;xsl:variable name="preloc"&gt;&#xD;
                &lt;xsl:value-of select="./preceding-sibling::marc:datafield[@tag='999'][1]/marc:subfield[@code='l']"/&gt;&#xD;
