                    &lt;xsl:with-param  name="input" select="marc:subfield[@code='b']" /&gt;&#xD;
                    &lt;xsl:with-param  name="characters"&gt;,-./ :;&lt;/xsl:with-param&gt;&#xD;
                    &lt;/xsl:call-template&gt;&#xD;
                &lt;/xsl:if&gt;&#xD;
                &lt;xsl:if test="marc:subfield[@code='h']"&gt;&#xD;
