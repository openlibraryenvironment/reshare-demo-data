                            &lt;/classificationNumber&gt;&#xD;
                            &lt;classificationTypeId&gt;9075b5f8-7d97-49e1-a431-73fdd468d476&lt;/classificationTypeId&gt; &lt;!-- SUDOC --&gt;&#xD;
                        &lt;/xsl:when&gt;&#xD;
                        &lt;/xsl:choose&gt;&#xD;
