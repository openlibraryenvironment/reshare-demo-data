                                &lt;xsl:if test="position() &gt; 1"&gt;&#xD;
                                &lt;xsl:text&gt;; &lt;/xsl:text&gt;&#xD;
                            &lt;/xsl:if&gt;&#xD;
                            &lt;xsl:value-of select="."/&gt;&#xD;
                            &lt;/xsl:for-each&gt;&#xD;
