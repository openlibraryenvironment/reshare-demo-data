                              &lt;xsl:when test=&quot;marc:subfield[@code=&#39;t&#39;]=&#39;OTHER&#39;&quot;     &gt;04858422-c46f-4041-94fc-f5c5a6b98319&lt;/xsl:when&gt; &lt;!-- Other --&gt;&#xD;
