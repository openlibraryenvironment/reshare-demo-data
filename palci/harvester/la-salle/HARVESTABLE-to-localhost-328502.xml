                  &lt;/i&gt;&#xD;
                &lt;/xsl:for-each&gt;&#xD;
                &lt;/arr&gt;&#xD;
              &lt;/items&gt;&#xD;
            &lt;/i&gt;&#xD;
          &lt;/xsl:if&gt;&#xD;
        &lt;/xsl:for-each&gt;&#xD;
      &lt;/xsl:if&gt;&#xD;
      &lt;xsl:if test="not(marc:datafield[@tag='945'])"&gt;&#xD;
