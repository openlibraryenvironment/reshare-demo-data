                      &lt;!-- Mapping from leader position 6 --&gt;&#xD;
                      &lt;xsl:choose&gt;&#xD;
                      &lt;xsl:when test="$bl='s' or $bl='b'"&gt;191da404-dec2-4977-9770-1443d9d6c238&lt;/xsl:when&gt; &lt;!-- CNR --&gt;&#xD;
