                &lt;xsl:copy-of select="@*"/&gt;&#xD;
                &lt;xsl:copy-of select="*"/&gt;&#xD;
            &lt;/xsl:copy&gt;&#xD;
        &lt;/original&gt;&#xD;
&#xD;
   &lt;/xsl:when&gt;&#xD;
   &lt;xsl:otherwise&gt;&#xD;
        &lt;delete&gt;&#xD;
